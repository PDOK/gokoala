{
  "name": "view-component",
  "version": "1.0.1",
  "type": "module",
  "files": [
    "dist/view-component/browser/"
  ],
  "typings": "view-component.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "npm run format && npm run lint && ng build --configuration production --verbose --output-hashing none && npm pack .",
    "watch": "ng build --watch --configuration development",
    "test": "npx cypress info && npx cypress run --component",
    "ghpage": "angular-cli-ghpages --dir=dist/view-component/browser",
    "lint": "ng lint",
    "cypress": "export CYPRESS_REMOTE_DEBUGGING_PORT=9222; npx cypress open",
    "format": "npx prettier --write .",
    "upgrade": "ng update @angular/cli @angular/core angular-cli-ghpages @angular-devkit/build-angular @angular/elements @angular-eslint/eslint-plugin @angular-eslint/eslint-plugin-template @angular-eslint/schematics --force && npm update && npm audit fix --force",
    "bundle-size": "ng build && esbuild-visualizer --metadata ./dist/view-component/stats.json",
    "apigen1": "ng-openapi-gen --input https://api.pdok.nl/bzk/location-api/autocomplete/v1-preprod/api?f=json --output ./src/app/api",
    "apigen2-no-deepobject-yet": "npx @openapitools/openapi-generator-cli generate -i https://api.pdok.nl/bzk/location-api/autocomplete/v1-preprod/api?f=json -g typescript -o ./src/app/api1"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "21.0.5",
    "@angular/compiler": "21.0.5",
    "@angular/core": "21.0.5",
    "@angular/elements": "21.0.5",
    "@angular/platform-browser": "21.0.5",
    "@angular/platform-browser-dynamic": "21.0.5",
    "ngx-logger": "^5.0.12",
    "ol": "10.5.0",
    "ol-mapbox-style": "12.6.0",
    "proj4": "2.19.5",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/build": "^21.0.3",
    "@angular/cli": "^21.0.3",
    "@angular/compiler-cli": "21.0.5",
    "@openapitools/openapi-generator-cli": "^0.0.6",
    "@types/jest": "^29.5.12",
    "@types/proj4": "2.5.5",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "angular-cli-ghpages": "2.0.3",
    "axe-core": "4.11.0",
    "cypress": "15.4.0",
    "cypress-axe": "1.7.0",
    "cypress-network-idle": "^1.14.2",
    "cypress-vite": "^1.5.0",
    "esbuild-visualizer": "^0.6.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.2",
    "eslint-plugin-prettier": "5.1.3",
    "ng-openapi-gen": "^0.52.0",
    "openapi-typescript": "^7.4.4",
    "prettier": "3.3.1",
    "prettier-eslint": "16.3.0",
    "typescript": "^5.9.3"
  }
}
